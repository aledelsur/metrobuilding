<p id="notice"><%= notice %></p>

<h1>Propiedades</h1>

<table class="table table-bordered">
  <thead>
    <tr>
      <th>Categoría</th>
      <th>Propietario</th>
      <!-- <th colspan="3"></th> -->
    </tr>
  </thead>

  <tbody>
    <% @properties.each do |property| %>
      <tr>
        <td><%= property.property_category_id %></td>
        <td><%= property.user_id %></td>
        <td><%= link_to 'Ver', property %></td>
        <td><%= link_to 'Editar', edit_property_path(property) %></td>
        <td><%= link_to 'Eliminar', property, method: :delete, data: { confirm: 'Estás seguro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

